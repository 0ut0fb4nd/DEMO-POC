<%- include ("partials/nav") %>

<div class="container contact">
  <%- include ("partials/messages") %>
    <form action="/contactus" method="POST">
        <div class="form-group">
            <label for="inputName">Name</label>
            <input type="text" class="form-control" name="contactName" placeholder="Enter Your Name">
          </div>
        <div class="form-row">
          <div class="form-group col-md-6">
            <label for="inputEmail4">Email</label>
            <input type="email" class="form-control"  name="contactEmail" placeholder="Email" >
          </div>
          <div class="form-group col-md-6">
            <label type="number" for="inputphone">Phone No</label>
            <input type="Number" class="form-control"  name="contactPhone" placeholder="Phone No" >
          </div>
        </div>
        
        <div class="form-group">
            <label for="exampleFormControlTextarea1">Comments</label>
            <textarea class="form-control"  name="contactTextArea" rows="3" ></textarea>
          </div>
          <button type="submit" class="btn btn-primary btn-block">
            Submit
          </button>
          <!-- <a class="btn btn-primary btn-lg btn-block" href="/contactus" role="button">Submit</a> -->
        <!-- <button type="submit" href="/users/logout" class="btn btn-primary btn-lg btn-block">Submit</button> -->
      </form>
</div>